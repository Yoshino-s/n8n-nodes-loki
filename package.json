{
  "name": "n8n-nodes-loki",
  "version": "0.1.0",
  "description": "",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/yoshino-s/n8n-nodes-loki.git",
  "author": {
    "name": "Yoshino-s",
    "email": "yoshino.prog@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yoshino-s/n8n-nodes-loki.git"
  },
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "build": "swc src -d dist && ts-node ./scripts/copy.ts",
    "build:watch": "ts-node ./scripts/copy.ts && swc src -d dist --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepublishOnly": "yarn build",
    "release": "standard-version",
    "commit": "git-cz",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/Loki/Loki.node.js"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.27",
    "@swc/jest": "^0.2.24",
    "@types/glob": "^8.0.1",
    "@types/jest": "^29.2.6",
    "@types/request-promise-native": "^1.0.18",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "chokidar": "^3.5.3",
    "commitizen": "^4.2.6",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "grpc-tools": "^1.12.4",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "prettier": "^2.8.3",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "standard-version": {
    "bumpFiles": [
      "package.json"
    ],
    "header": "# Changelog\n\n"
  },
  "dependencies": {
    "glob": "^8.1.0",
    "n8n-core": "^0.152.0",
    "n8n-workflow": "^0.134.0"
  }
}
